# Ethereum RPC endpoint
ETH_RPC_URL=https://eth.llamarpc.com

# Logging level: trace, debug, info, warn, error
RUST_LOG=info

# Server configuration (TCP mode)
# SERVER_HOST: Server bind address (127.0.0.1, localhost, 0.0.0.0, etc.)
# Default: 127.0.0.1
SERVER_HOST=127.0.0.1
# SERVER_PORT: Server bind port (0 = use stdio mode, >0 = TCP mode)
# Default: 0 (stdio mode for MCP standard)
SERVER_PORT=0

# Vitalik's wallet address
WALLET_ADDRESS=0xd8dA6BF26964aF9D7eEd9e03E53415D37aA96045
# USDC contract addr
TOKEN_ADDRESS=0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48
